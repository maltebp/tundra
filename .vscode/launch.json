{

    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "name": "Emulator Tests (Debug)",
            "target": "localhost:3333",
            "remote": true,
            "executable": "build\\playstation\\tests\\Debug\\td.playstation.tests.elf",
            "gdbpath": "gdb-multiarch",
            "cwd": "${workspaceRoot}",
            "autorun": [
                "set substitute-path /project .",
                "monitor reset shellhalt",
                "load build/playstation/tests/Debug/td.playstation.tests.elf",
            ],
            "valuesFormatting": "prettyPrinters"
        },
        {
            "type": "gdb",
            "request": "attach",
            "name": "Hardware Tests (Debug)",
            "target": "localhost:3334",
            "remote": true,
            "executable": "build\\playstation\\tests\\Debug\\td.playstation.tests.elf",
            "gdbpath": "gdb-multiarch",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "prettyPrinters",
            "autorun": [
                "load build/playstation/tests/Debug/td.playstation.tests.elf",
            ],
        },
    ],
}